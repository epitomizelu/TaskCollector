{
  "cli": { "version": ">= 3.0.0" },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "image": "latest",
        "withoutCredentials": false
      },
      "env": {
        "EXPO_PUBLIC_API_KEY": "${EXPO_PUBLIC_API_KEY}"
      }
    },
    "production": {
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "image": "latest",
        "withoutCredentials": false
      },
      "env": {
        "EXPO_PUBLIC_API_KEY": "${EXPO_PUBLIC_API_KEY}"
      }
    }
  },
  "submit": { "production": {} }
}


